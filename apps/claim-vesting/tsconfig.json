{
  "extends": "@bleu/tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "#/*": ["./*"],
      "@bleu/cow-hooks-ui": ["../../../packages/hooks-ui/src"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
  "types": ["node", "jest", "@testing-library/jest-dom"]
}
